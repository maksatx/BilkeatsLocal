<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/transparent_background"
    tools:context=".pianopage.classical_music.ClassicalPianoPage">

    <ImageView
        android:id="@+id/backgroundRonesans"
        android:layout_width="478dp"
        android:layout_height="match_parent"
        android:alpha="0.7"
        android:background="#B3F1E0E0"
        android:onClick="goToClassic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/disco" />

    <Button
        android:id="@+id/button4"
        android:layout_width="287dp"
        android:layout_height="66dp"
        android:alpha="0.8"
        android:background="@drawable/forbutton"
        android:bufferType="normal"
        android:fontFamily="@font/disco"
        android:onClick="popMusic1"
        android:text="My Heath Will Go On\nCeline Dion"
        android:textAllCaps="false"
        android:textSize="60px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bilkeatsLogo"
        app:layout_constraintVertical_bias="0.088" />

    <Button
        android:id="@+id/button2"
        android:layout_width="287dp"
        android:layout_height="66dp"
        android:alpha="0.8"
        android:background="@drawable/forbutton"
        android:bufferType="normal"
        android:fontFamily="@font/disco"
        android:onClick="popMusic2"
        android:text="Lovely\nBillie Elish and Khalid"
        android:textAllCaps="false"
        android:textSize="60px"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bilkeatsLogo"
        app:layout_constraintVertical_bias="0.278" />

    <ImageView
        android:id="@+id/bilkeatsLogo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bilkeatslogo" />

    <Button
        android:id="@+id/button"
        android:layout_width="287dp"
        android:layout_height="66dp"
        android:alpha="0.8"
        android:background="@drawable/forbutton"
        android:bufferType="normal"
        android:fontFamily="@font/disco"
        android:onClick="popMusic4"
        android:text="All The Things She Said\nt.A.T.u."
        android:textAllCaps="false"
        android:textSize="60px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bilkeatsLogo"
        app:layout_constraintVertical_bias="0.647" />

    <Button
        android:id="@+id/button5"
        android:layout_width="287dp"
        android:layout_height="66dp"
        android:alpha="0.8"
        android:background="@drawable/forbutton"
        android:bufferType="normal"
        android:fontFamily="@font/disco"
        android:onClick="popMusic5"
        android:text="Love Is All Around\nWet Wet Wet"
        android:textAllCaps="false"
        android:textSize="60px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bilkeatsLogo"
        app:layout_constraintVertical_bias="0.841" />

    <Button
        android:id="@+id/button3"
        android:layout_width="287dp"
        android:layout_height="66dp"
        android:alpha="0.8"
        android:background="@drawable/forbutton"
        android:bufferType="normal"
        android:fontFamily="@font/disco"
        android:onClick="popMusic3"
        android:text="Another Love\nTom Odell"
        android:textAllCaps="false"
        android:textSize="60px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bilkeatsLogo"
        app:layout_constraintVertical_bias="0.468" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:background="#00FFFFFF"
        android:onClick="goToClassic"
        app:layout_constraintBottom_toTopOf="@+id/button4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toEndOf="@+id/bilkeatsLogo"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.3"
        app:srcCompat="@drawable/baseline_arrow_forward_24" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:background="#00FFFFFF"
        android:onClick="goToRock"
        app:layout_constraintBottom_toTopOf="@+id/button4"
        app:layout_constraintEnd_toStartOf="@+id/bilkeatsLogo"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/backgroundRonesans"
        app:layout_constraintVertical_bias="0.307"
        app:srcCompat="@drawable/baseline_arrow_back_24" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/nav"
        android:layout_width="wrap_content"
        android:background="@drawable/transparent_background"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/menu_bar">

    </com.google.android.material.bottomnavigation.BottomNavigationView>


</androidx.constraintlayout.widget.ConstraintLayout>